{
	"navigationFallback": {
		"rewrite": "/index.html",
		"exclude": ["/assets/*", "*.js", "*.css", "*.map", "*.json"]
	},
	"mimeTypes": {
		".js": "text/javascript",
		".mjs": "text/javascript",
		".css": "text/css",
		".json": "application/json"
	},
	"globalHeaders": {
		"X-Content-Type-Options": "nosniff"
	},
	"routes": [
		{
			"route": "/assets/*",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			}
		},
		{
			"route": "*.js",
			"headers": {
				"content-type": "text/javascript; charset=utf-8",
				"cache-control": "public, max-age=31536000, immutable"
			}
		},
		{
			"route": "*.css",
			"headers": {
				"content-type": "text/css; charset=utf-8",
				"cache-control": "public, max-age=31536000, immutable"
			}
		},
		{
			"route": "/api/*",
			"allowedRoles": ["anonymous"]
		}
	]
}
