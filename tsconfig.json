{
	"compilerOptions": {
		// DO NOT EDIT
		"outDir": "./dist",
		"target": "es6",
		"module": "Node16",
		"sourceMap": true,
		"declaration": true,
		"declarationMap": true,
		// Set your preferences here for TypeScript
		"strict": true,
		"jsx": "react",
		"moduleResolution": "Node16",
		"lib": ["DOM", "ESNext"],
		"types": ["vite/client"],
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
